<table class="table text-center">
    <thead>
    <tr>
        <th class="active text-center">Title</th>
        <th class="active text-center">Action</th>
    </tr>
    </thead>

    <tbody>
    {% for resume in resumes %}
        <tr>
            <td>{{ resume.title }}</td>
            <td>
                <a class="fa fa-check" href="{{ path('resume_confirm', {job_id: job_id, resume_id:resume.id}) }}"></a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>